
public class revstr {

	public static void main(String[] args) {
		
		String s2="reverse it";
		
		for(int i=s2.length()-1;i>=0;i--) {
			System.out.print(s2.charAt(i));
			
			char s= s2.charAt(i);
			{
					System.out.println( "it is a palindrome");
				
			} 
				System.out.println("not a palindrome");
		}
