
public class Palindrome {

	public static void main(String[] args) {
		
		String s="malayalam";
		String t="";
		
		for(int i=s.length()-1;i>=0;i--) {
			t=t+s.charAt(i);
		}
		System.out.println(t);
		if(t==s) {
			System.out.println( "String is palindrome");
		}
		// TODO Auto-generated method stub

	}

}
